<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

	<module name="account" package="com.axelor.apps.account.db" />

	<entity name="InvoiceLine" lang="java">

		<string name="vatDisplayName" />
		<string name="invoicedQty" />
		<string name="leftToDeliveredQty" />
		<string name="leftToInvoicedQty" />
		<string name="vatId" />
		<boolean name="isPercentageDiscount" default="false" />
		<decimal name="vatDisplayRate" />
		<decimal name="deliveredAmount" />

	</entity>

</domain-models>
