<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

	<module name="base" package="com.axelor.apps.base.db" />

	<entity name="Partner" lang="java">

		<string name="supplierRef" />
		<string name="lastName" />
		<boolean name="isSameAddressOk" default="false" />
		<string name="customerTitle" />
		<string name="customerFirstName" />
		<string name="customerLastName" />
		<string name="customerCompanyName" />
		<string name="customerPhone" />
		<string name="customerEmail" />
		<string name="customerDeliveryAddress" large="true" />
		<string name="customerInvoicingAddress" large="true" />
		<string name="siret" />
		<string name="vatIntraCommunity" />
		<string name="partnerComment" large="true" />
		<string name="purchaseVatId" />
		<string name="contactId" />
		<string name="salesVatId" />

	</entity>

</domain-models>
